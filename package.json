{
  "name": "questioner",
  "version": "1.0.0",
  "description": "A meetup App",
  "main": "server/index.js",
  "scripts": {
    "start": "nodemon --exec babel-node server/index.js",
    "test": "nyc mocha --require babel-register server/tests/*.js --exit",
    "db_migrate": "babel-node ./server/migrations/createtables.js",
    "db_drop": "babel-node ./server/database/droptables.js",
    "db_seed": "babel-node ./server/database/seed.js",
    "build": "babel server -d dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toluola/Questioner.git"
  },
  "author": "Tolu' Olaniyan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/toluola/Questioner/issues"
  },
  "homepage": "https://github.com/toluola/Questioner#readme",
  "nyc": {
    "exclude": [
      "UI/**.js",
      "**/database/**",
      "**/controllers/**",
      "**/helpers/**",
      "**/**.spec.js"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cloudinary": "^1.13.2",
    "cors": "^2.8.5",
    "coveralls": "^3.0.2",
    "datauri": "^1.1.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.11.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "husky": "^1.3.1",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.4.0",
    "lint-staged": "^13.1.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "multer": "^1.4.1",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "pgtools": "^0.3.0",
    "sequelize": "^5.8.5",
    "sequelize-cli": "^5.4.0",
    "sequelize-pg-utilities": "^1.0.8",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "prettier": "^1.15.3"
  }
}
