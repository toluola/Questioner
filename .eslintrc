{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "plugins": ["prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2016,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-console": 0,
        "import/prefer-default-export": "off",
         "no-unused-vars": 0,
        "arrow-body-style": 0,
        "no-confusing-arrow": 0,
        "valid-typeof": 0,
        "no-return-assign": 0,
        "no-undef": 0,
        "camelcase": 0,
        "consistent-return": 0,
        "no-restricted-globals": 0,
        "prefer-destructuring": 0  
    }
}